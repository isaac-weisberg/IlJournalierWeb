  "compilerOptions": {
    "lib": [
      "ES2022.Error",
      "DOM"
    ],
    "outDir": "./build/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es6",
    "allowJs": true,
    "moduleResolution": "node",
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitThis": true
  }
}